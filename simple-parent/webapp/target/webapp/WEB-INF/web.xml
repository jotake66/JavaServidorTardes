<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>WeatherServlet</servlet-name>
  	<display-name>WeatherServlet</display-name>
  	<description></description>
  	<servlet-class>org.sonatype.mavenbook.web.WeatherServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>WeatherControladorServlet</servlet-name>
  	<display-name>WeatherControladorServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.controladores.WeatherControladorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>WeatherServlet</servlet-name>
  	<url-pattern>/weather</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WeatherControladorServlet</servlet-name>
  	<url-pattern>/w</url-pattern>
  </servlet-mapping>
</web-app>
