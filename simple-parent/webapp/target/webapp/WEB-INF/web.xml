<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	      version="3.0">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>WeatherServlet</servlet-name>
  	<servlet-class>org.sonatype.mavenbook.web.WeatherServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>WeatherControladorServlet</servlet-name>
  	<servlet-class>com.ipartek.formacion.controladores.WeatherControladorServlet</servlet-class>
  </servlet>
 
  <servlet-mapping>
  	<servlet-name>WeatherServlet</servlet-name>
  	<url-pattern>/weather</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WeatherControladorServlet</servlet-name>
  	<url-pattern>/w</url-pattern>
  </servlet-mapping>
 </web-app>
